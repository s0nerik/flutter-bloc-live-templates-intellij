<templateSet group="Fluter: built_value">
  <template name="built" value="abstract class $CLASS_NAME$ implements Built&lt;$CLASS_NAME$, $CLASS_NAME$Builder&gt; {&#10;  $CLASS_NAME$._();&#10;  factory $CLASS_NAME$([updates($CLASS_NAME$Builder b)]) = _$$$CLASS_NAME$;&#10;  static Serializer&lt;$CLASS_NAME$&gt; get serializer =&gt; _$$$CLASS_NAME_LOWER_CAMEL$Serializer;&#10;}" description="New BuiltValue class" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CLASS_NAME_LOWER_CAMEL" expression="camelCase(CLASS_NAME)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="DART_TOPLEVEL" value="true" />
    </context>
  </template>
  <template name="bser" value="static Serializer&lt;$CLASS_NAME$&gt; get serializer =&gt; _$$$CLASS_NAME_LOWER_CAMEL$Serializer;" description="New BuiltValue serializer" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="suggestVariableName()" defaultValue="" alwaysStopAt="false" />
    <variable name="CLASS_NAME_LOWER_CAMEL" expression="camelCase(CLASS_NAME)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="DART" value="true" />
      <option name="DART_STATEMENT" value="false" />
      <option name="DART_TOPLEVEL" value="false" />
    </context>
  </template>
</templateSet>